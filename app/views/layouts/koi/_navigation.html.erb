<ul class="non-pane">
  <li><%= link_to "View site", "/" -%></li>
  <li><%= link_to "Dashboard", koi_engine.dashboard_path %></li>
  <li><%= link_to "Sitemap", koi_engine.sitemap_nav_items_path %></li>
  <%- Koi::Menu.items.each do |menu_name,menu_items| -%>
    <li>
      <a href="#"><%= menu_name -%></a>
      <ul>
        <%- menu_items.each do |item_name,item_url| -%>
          <%= render "layouts/koi/navigation_item", item_name: item_name, item_url: item_url -%>
        <%- end -%>
        <%- if menu_name.to_s.eql?("Advanced") -%>
          <li><%= link_to "Clear Cache", koi_engine.clear_cache_path, method: :post %></li>
          <%- if current_admin.god? -%>
            <li><%= link_to "Background Jobs", koi_engine.sidekiq_path, target: "_blank" %></li>
          <%- end -%>
        <%- end -%>
      </ul>
    </li>
  <%- end -%>
  <li>
    <a href="#">Assets</a>
    <ul>
      <li><%= link_to "Images", koi_engine.new_image_path %></li>
      <li><%= link_to "Documents", koi_engine.new_document_path %></li>
    </ul>
  </li>
  <li><%= link_to "Settings", koi_engine.translations_path %></li>
  <li><%= link_to "Help", koi_engine.help_path %></li>
  <%- if current_admin.god? -%>
    <li><%= link_to "Developer Styleguide", koi_engine.styleguide_path %></li>
  <%- end -%>
  <li><%= link_to "Logout", koi_engine.destroy_admin_session_path, method: :delete %></li>
</ul>
