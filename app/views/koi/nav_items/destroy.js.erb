$.magnificPopup.close();

$('#nav-item-<%= resource.id %>').fadeOut('slow', function(){
  var $oldNode = $(this);
  // Deal with show/hide toggles
  var $parent = $oldNode.parent("ol");
  $oldNode.remove();
  if($parent.children().length === 0) {
    $parent.parent("li").removeClass("mjs-nestedSortable-branch mjs-nestedSortable-expanded");
    $parent.remove();
  }
});