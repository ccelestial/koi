<script type="text/javascript">
  $(function () {
    $(document).on('click', "[data-get-script]", function(e) {
      e.preventDefault();
      $.getScript(this.href);
    });
    $(".form-search input").keyup(function() {
      $.get($(".form-search").attr("action"), $(".form-search").serialize(), null, "script");
      return false;
    });
  })
</script>
